{
    "version": 2,
    "builds": [
        {
            "src": "static/**",
            "use": "@vercel/static"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://weekly.fee.red/api/$1"
        },
        {
            "source": "/",
            "destination": "/static/ranking.html"
        },
        {
            "source": "/(.*)",
            "destination": "/static/$1"
        }
    ]
}